[
  {
    "key": "shift+cmd+a",
    "command": "editor.emmet.action.wrapWithAbbreviation"
  },
  {
    "key": "ctrl+shift+k",
    "command": "editor.emmet.action.removeTag"
  },
  {
    "key": "ctrl+cmd+0",
    "command": "extension.ecsstractor_port_run"
  },
  {
    "key": "ctrl+cmd+-",
    "command": "extension.ecsstractor_port_runwithbem"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.terminal.focus",
    "when": "terminalIsOpen && editorTextFocus"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.files.action.focusFilesExplorer",
    "when": "explorerViewletVisible && openEditorsFocus && !inputFocus"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.files.action.focusOpenEditorsView",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft",
    "when": "editorIsOpen"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.explorer.fileView.focus",
    "when": "!editorIsOpen"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.navigateLeft",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.navigateRight",
    "when": "editorIsOpen"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.terminal.focus",
    "when": "terminalIsOpen && !editorIsOpen"
  },
  {
    "key": "ctrl+f",
    "command": "workbench.files.action.createFolderFromExplorer",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "ctrl+i",
    "command": "workbench.files.action.createFileFromExplorer",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "backspace",
    "command": "deleteFile",
    "when": "explorerViewletVisible && filesExplorerFocus && !explorerResourceReadonly && !inputFocus"
  },
  {
    "key": "ctrl+c",
    "command": "workbench.files.action.collapseExplorerFolders",
    "when": "explorerViewletVisible && filesExplorerFocus && !inputFocus"
  },
  {
    "key": "ctrl+e",
    "command": "workbench.files.action.refreshFilesExplorer"
  },
  {
    "key": "ctrl+[",
    "command": "workbench.action.decreaseViewWidth",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+]",
    "command": "workbench.action.increaseViewWidth",
    "when": "editorFocus"
  }
]
